<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="android.events.dg.actionSet">
         <menu
               label="EventsDG"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               class="android.events.dg.actions.ExtractorAction"
               icon="icons/sample.gif"
               id="android.events.dg.actions.ExtractorAction"
               label="Generate"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               tooltip="Extract events">
         </action>
      </actionSet>
   </extension>

</plugin>
